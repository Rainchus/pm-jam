#new:Function $ScrAPI_getFacingDirection {
	PUSH s0, ra
	LIO s0, .PlayerStatus
	LI t0, $facingLeftText
	%LWC1 f0, 00A8 (s0) %spriteFacingAngle
	LWC1 f0, 0080 (s0) %targetYaw
	LIF f2, 180.0
	C.LT.S f0, f2
	BC1T .facingRight
	ORI a2, r0, 0 %facing right value
	%facing left
	ORI a2, r0, 1 %facing left value
	.facingRight
	LW t0, 000C (a0)
	JAL ~Func:set_variable
	LW a1, 0000 (t0)

	POP s0, ra
	JR RA
	ORI v0, r0, .scrApi_Done2
}

#new:Function $clearHammerThrowVirtualEntityIndex {
	ADDIU t0, r0, FFFF
	SAW t0, $hammerThrowVirtualEntityIndex
	JR RA
	ORI v0, r0, 2
}

#new:Function $storeHammerThrowVirtualEntityIndex {
	PUSH ra
	LW t0, 000C (a0)
	JAL ~Func:get_variable
	LW a1, 0000 (t0)
	SAW v0, $hammerThrowVirtualEntityIndex
	POP ra
	JR RA
	ORI v0, r0, 2
}

#new:Script:Map $hammerThrowOverworld {
    Call $setHammerThrowBool ( .True )
    Call $getHammerUpgradeValue ( *Var1 )
    Switch  *Var1
    Case  ==  .Item:Nothing % 0
		%Call  SetPlayerAnimation  ( 00040017 )
		%Call  SetAnimation (.Actor:Player 00000000 00040017)
		Wait 4`
		%Call  SetPlayerAnimation  ( 0004001B )
		%Call  SetAnimation (.Actor:Player 00000000 0004001B)
		Wait 4`
        Call  CreateVirtualEntity   ( *VarA $EntityModelScript_802A3CE4 )
    Case  ==  .Item:Jump % 1
		%Call  SetPlayerAnimation  ( 0004001D )
		%Call  SetAnimation (.Actor:Player 00000000 0004001D)
		Wait 4`
		%Call  SetPlayerAnimation  ( 00040021 )
		%Call  SetAnimation (.Actor:Player 00000000 00040021)
		Wait 4`
        Call  CreateVirtualEntity   ( *VarA $EntityModelScript_802A3D00 )
    Case  ==  .Item:SpinJump % 2
		%Call  SetPlayerAnimation  ( 00040023 )
		%Call  SetAnimation (.Actor:Player 00000000 00040023)
		Wait 4`
		%Call  SetPlayerAnimation  ( 00040027 )
		%Call  SetAnimation (.Actor:Player 00000000 00040027)
		Wait 4`
        Call  CreateVirtualEntity   ( *VarA $EntityModelScript_802A3D1C )
    EndSwitch
	Call  $storeHammerThrowVirtualEntityIndex ( *VarA )
    Call  GetPlayerPos ( *Var0 *Var1 *Var2 )
	Call  $ScrAPI_getFacingDirection ( *Var3 )
	If *Var3 == 0 %if facing right
	    Add   *Var0  00000014
    	Add   *Var1  0000002A
    	Add   *Var2  00000005
    	Call  SetVirtualEntityPosition  ( *VarA *Var0 *Var1 *Var2 )
		Add *Var0 100`
		Set *Var5 FFFFFFC4
	Else
	    Add   *Var0  FFFFFFEC
    	Add   *Var1  0000002A
    	Add   *Var2  00000005
    	Call  SetVirtualEntityPosition ( *VarA *Var0 *Var1 *Var2 )
		Sub *Var0 100`
		Set *Var5 0000003C
    EndIf
	Call  SetVirtualEntityScale     ( *VarA *Fixed[0.8] *Fixed[0.8] *Fixed[0.8] )

    Thread
        Set   *Var6  00000000
        Loop
            Add   *Var6  *Var5
            Call  SetVirtualEntityRotation  ( *VarA 00000000 00000000 *Var6 )
            Wait  1`
        EndLoop
    EndThread

	%check if entity should be deleted due to hitting switch
	Thread
		Loop
			Call $ScrAPI_checkIfSwitchHit ( *Var7 )
			If *Var7 == 1 %switch has been hit
				Call  $clearHammerThrowVirtualEntityIndex
				Call  $setHammerThrowBool ( .False )
				Call $ScrApi_SetSwitchHitBool ( .False )
				Return
			EndIf
			Wait 1`
		EndLoop
	EndThread

    Call  SetVirtualEntityJumpGravity   ( *VarA *Fixed[1.3] )
    Add   *Var2  00000005
	Add   *Var1  FFFFFFC0 %-40
    Call  VirtualEntityJumpTo   ( *VarA *Var0 *Var1 *Var2  28` )
    %Call  VirtualEntityLandJump ( *VarA )
    Call  DeleteVirtualEntity   ( *VarA )
	Call  $clearHammerThrowVirtualEntityIndex
    Call  $setHammerThrowBool ( .False )
    Return
    End
}

